# 开发环境配置

# 环境标识
NODE_ENV=development
VITE_ENV=dev

# API 基础路径
VITE_API_BASE_URL=https://dev-api.example.com/api

# 是否启用 Mock 数据
VITE_USE_MOCK=false

# 是否开启代理
VITE_USE_PROXY=false

# 上传文件地址
VITE_UPLOAD_URL=https://dev-api.example.com/api/upload

# WebSocket 地址
VITE_WS_URL=wss://dev-api.example.com/ws

# 是否显示开发工具
VITE_SHOW_DEV_TOOLS=true

# 是否开启 vconsole（移动端调试）
VITE_USE_VCONSOLE=false
